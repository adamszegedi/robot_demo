root = true

[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
charset = utf-8
end_of_line = crlf

[*.bat]
indent_style = tab
end_of_line = crlf

[LICENSE]
insert_final_newline = false

[Makefile]
indent_style = tab

[*.yaml]
indent_style = space
indent_size = 2
insert_final_newline = false

[*.robot]
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
